{
  "compileOnSave": false,
  "compilerOptions": {
    "jsx": "react-jsx",
    "rootDir": ".",
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "types": [
      "jest",
      "cypress"
    ],
    "paths": {
      "@mussia12/buildable-header": ["libs/buildable-header/src/index.ts"],
      "@mussia12/fullstack/api-modules": [
        "libs/fullstack/api-modules/src/index.ts"
      ],
      "@mussia12/fullstack/event1-api-nest-module": [
        "libs/fullstack/event1-api-nest-module/src/index.ts"
      ],
      "@mussia12/fullstack/event2-api-nest-module": [
        "libs/fullstack/event2-api-nest-module/src/index.ts"
      ],
      "@mussia12/fullstack/event3-api-nest-module": [
        "libs/fullstack/event3-api-nest-module/src/index.ts"
      ],
      "@mussia12/fullstack/health-api-nest-module": [
        "libs/fullstack/health-api-nest-module/src/index.ts"
      ],
      "@mussia12/fullstack/mui-theme-provider": [
        "libs/fullstack/mui-theme-provider/src/index.ts"
      ],
      "@mussia12/fullstack/projects-api-nest-module": [
        "libs/fullstack/projects-api-nest-module/src/index.ts"
      ],
      "@mussia12/fullstack/users-api-nest-module": [
        "libs/fullstack/users-api-nest-module/src/index.ts"
      ],
      "@mussia12/pulumi-gcp": ["libs/pulumi/gcp/src/index.ts"],
      "@mussia12/pulumi/aws": ["libs/pulumi/aws/src/index.ts"],
      "@mussia12/pulumi/shared-libs": ["libs/pulumi/shared-libs/src/index.ts"],
      "@mussia12/pulumi/shared-types": [
        "libs/pulumi/shared-types/src/index.ts"
      ],
      "@mussia12/shared/configs": ["libs/shared/configs/src/index.ts"],
      "@mussia12/shared/data-types": ["libs/shared/data-types/src/index.ts"],
      "@mussia12/shared/mongoose-schemas": [
        "libs/shared/mongoose-schemas/src/index.ts"
      ],
      "@mussia12/shared/mui": ["libs/shared/mui/src/index.ts"],
      "@mussia12/shared/object-utils": [
        "libs/shared/object-utils/src/index.ts"
      ],
      "@mussia12/shared/validations": ["libs/shared/validations/src/index.ts"],
      "mussia12/publishible-header": ["libs/publishible-header/src/index.ts"]
    },
  },
  "exclude": ["node_modules", "tmp"],
  "typedocOptions": {
    "entryPoints": [
      "libs", "apps",
//      "libs/buildable-header/src",
//      "libs/fullstack/api-modules/src",
//      "libs/pulumi/gcp/src"
    ],
    "out": "docs",
    "exclude": ["**/**/__tests__/*"]
    //    "json": "docs/json.json"
  }
}
